{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "networkInterfaceName": {
            "value": "studentvm987"
        },
        "enableAcceleratedNetworking": {
            "value": true
        },
        "networkSecurityGroupName": {
            "value": "StudentVM-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "RDP",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "3389"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "StudentSubnet"
        },
        "virtualNetworkName": {
            "value": "StudentVnet"
        },
        "addressPrefixes": {
            "value": [
                "192.168.0.0/24"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "StudentSubnet",
                    "properties": {
                        "addressPrefix": "192.168.0.248/29"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "StudentVM-ip"
        },
        "publicIpAddressType": {
            "value": "Static"
        },
        "publicIpAddressSku": {
            "value": "Standard"
        },
        "pipDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineName": {
            "value": "StudentVM"
        },
        "virtualMachineComputerName": {
            "value": "StudentVM"
        },
        "virtualMachineRG": {
            "value": "1-47370675-playground-sandbox"
        },
        "osDiskType": {
            "value": "Standard_LRS"
        },
        "osDiskDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineSize": {
            "value": "Standard_D2s_v3"
        },
        "nicDeleteOption": {
            "value": "Delete"
        },
        "adminUsername": {
            "value": "Student"
        },
        "adminPassword": {
            "value": null
        },
        "patchMode": {
            "value": "AutomaticByOS"
        },
        "enableHotpatching": {
            "value": false
        }
    }
}